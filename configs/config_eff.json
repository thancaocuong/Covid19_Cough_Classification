{
    "name": "Covid19-tresnet_m_448",
    "n_gpu": 2,
    "fp16": true,

    "arch": {
        "type": "FinetuneEfficientNet",
        "args": {"model_name": "tf_efficientnet_b4_ns",
                 "num_classes": 4,
                 "inchannels": 3,
                 "pretrained": true}
    },
    "do_pseudo": false,
    "unlabeled_dataset": {
        "csv_path": "/home/ubuntu/cuong_test/AICOVIDVN/dataset/aicv115m_public_test/metadata_public_test.csv",
        "audio_folder": "/home/ubuntu/cuong_test/AICOVIDVN/dataset/aicv115m_public_test/public_test_audio_files_8k/",
        "training_batch_size": 16,
        "num_workers": 10,
        "mfcc_config":
            {
                "n_mfcc": 15, "n_fft": 1024, "hop_length": 256, "max_samples": 60000
            }
    },
    "dataset": {
        "csv_path": "/media/sonnh/kaggle-SIIM-FISABIO-RSNA-COVID-19-Detection/data/train_merge_study_kfold2.csv",
        "images_dir": "/media/sonnh/kaggle-SIIM-FISABIO-RSNA-COVID-19-Detection/data/image",
        "input_size": 960,
        "training_batch_size": 8,
        "validate_batch_size": 16,
	"num_workers": 20
    },
    "data_loader": {
        "type": "MnistDataLoader",
        "args":{
            "data_dir": "data/",
            "batch_size": 128,
            "shuffle": true,
            "validation_split": 0.1,
            "num_workers": 2
        }
    },
    "optimizer": {
        "type": "Adam",
        "args":{
            "lr": 0.001,
            "weight_decay": 0.0001,
            "amsgrad": true
        }
    },
    "loss": "focal_binary_cross_entropy",
    "metrics": [
        "roc_auc"
    ],
    "lr_scheduler": {
        "type": "OneCycleLR",
        "args": {
            "pct_start": 0.1,
            "max_lr": 0.001,
            "div_factor": 25.0,
            "final_div_factor": 4.0,
            "steps_per_epoch": 1,
            "total_steps": 50

        }
    },
    "trainer": {
        "epochs": 50,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "max val_roc_auc",
        "early_stop": 50,
        "warmup": 5,
        "tensorboard": true
    }
}
